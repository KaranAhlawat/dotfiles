{
  "C_Cpp.clang_format_fallbackStyle": "GNU",
  "C_Cpp.default.cppStandard": "gnu++20",
  "calva.autoOpenREPLWindow": false,
  "calva.clojureLspPath": "/home/karan/.local/bin/clojure-lsp",
  "calva.useLiveShare": true,
  "cssvar.extensions": [
    "css",
    "scss",
    "sass",
    "less",
    "postcss",
    "vue",
    "svelte",
    "astro",
    "ts",
    "tsx",
    "js",
    "jsx"
  ],
  "cssvar.files": [
    "./node_modules/open-props/open-props.min.css",
    "./node_modules/open-props/media.min.css"
  ],
  "cssvar.ignore": [],
  "dev.containers.dockerComposePath": "podman-compose",
  "dev.containers.dockerPath": "podman",
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "block",
  "editor.fontFamily": "monospace",
  "editor.fontLigatures": "true",
  "editor.fontSize": 13,
  "editor.formatOnSave": true,
  "editor.linkedEditing": true,
  "editor.minimap.autohide": true,
  "editor.scrollbar.vertical": "hidden",
  "editor.tabSize": 2,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "astro", // Enable .astro
    "typescript", // Enable .ts
    "typescriptreact" // Enable .tsx
  ],
  "files.watcherExclude": {
    "**/.ammonite": true,
    "**/.bloop": true,
    "**/.metals": true
  },
  "liveshare.openSharedServers": false,
  "window.menuBarVisibility": "hidden",
  "window.titleBarStyle": "native",
  "workbench.editor.enablePreview": true,
  "workbench.iconTheme": "vscode-icons",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "terminal.integrated.env.linux": {
    "GRAALVM_HOME": "/home/karan/.sdkman/candidates/java/21-graalce"
  },
  "metals.fallbackScalaVersion": "2.13.12",
  "redhat.telemetry.enabled": false,
  "metals.superMethodLensesEnabled": false,
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "vim.leader": "<Space>",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-s>": false,
    "<C-z>": false,
    "<C-p>": false
  },
  "window.zoomLevel": 1,
  "workbench.colorTheme": "JetBrains New UI Dark Theme",
  "editor.renderWhitespace": "all"
}