{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "C_Cpp.clang_format_fallbackStyle": "GNU",
  "C_Cpp.default.cppStandard": "gnu++20",
  "cssvar.extensions": [
    "css",
    "scss",
    "sass",
    "less",
    "postcss",
    "vue",
    "svelte",
    "astro",
    "ts",
    "tsx",
    "js",
    "jsx"
  ],
  "cssvar.files": [
    "./node_modules/open-props/open-props.min.css",
    "./node_modules/open-props/media.min.css"
  ],
  "cssvar.ignore": [],
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "block",
  "editor.fontFamily": "monospace",
  "editor.fontLigatures": false,
  "editor.fontSize": 15,
  "editor.fontWeight": "500",
  "editor.formatOnSave": true,
  "editor.lineHeight": 2.5,
  "editor.linkedEditing": true,
  "editor.minimap.autohide": true,
  "editor.scrollbar.vertical": "hidden",
  "editor.tabSize": 2,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "astro", // Enable .astro
    "typescript", // Enable .ts
    "typescriptreact" // Enable .tsx
  ],
  "svelte.enable-ts-plugin": true,
  "window.menuBarVisibility": "hidden",
  "window.titleBarStyle": "native",
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "none",
  "zig.zigPath": "",
  "zig.zls.enabled": true,
  "zig.zls.path": "/home/karan/.config/Code/User/globalStorage/ziglang.vscode-zig/zls_install/zls",
  "workbench.colorTheme": "Nord",
  "window.zoomLevel": 1,
  "dev.containers.dockerPath": "podman",
  "dev.containers.dockerComposePath": "podman-compose"
}
